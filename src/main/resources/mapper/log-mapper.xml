<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "mybatis-3-mapper.dtd">

<mapper namespace="Log">
    
    <!-- com.kdb.common.dto.LogLoginDTO Result Map 정의 -->
    <resultMap id="LogLoginResultMap" type="com.kdb.common.dto.LogLoginDTO">
        <result property="empNo" column="EMP_NO"/>
        <result property="type" column="TYPE"/>
        <result property="result" column="RESULT"/>
        <result property="createdTime" column="CREATED_TIME"/>
    </resultMap>
    
    <!-- Login Log Insert 쿼리 -->
    <insert id="saveLoginLog" parameterType="logLogin">
        insert into LOG_LOGIN(EMP_NO, TYPE, RESULT)
        values (#{empNo}, #{type}, #{result})
    </insert>  
    
    <!-- Login Log Select 쿼리 -->
    <select id="findLoginLogAll" resultMap="LogLoginResultMap">
        select * from LOG_LOGIN
    </select>  
    
</mapper>