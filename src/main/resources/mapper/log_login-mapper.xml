<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "mybatis-3-mapper.dtd">

<mapper namespace="LogLogin">
    
    <!-- com.kdb.common.dto.LogLoginDTO Result Map 정의
    <resultMap id="LogLoginResultMap" type="com.kdb.com.dto.LogLoginDTO">
        <result property="empNo" column="EMP_NO"/>
        <result property="type" column="TYPE"/>
        <result property="result" column="RESULT"/>
        <result property="createdTime" column="CREATED_TIME"/>
    </resultMap>
    -->
    
    <!-- Login Log Insert 쿼리 -->
    <insert id="saveLoginLog" parameterType="logLogin">
        insert into LOG_LOGIN(empNo, type, result, createdTime)
        values (#{empNo}, #{type}, #{result}, NOW())
    </insert>  
    
    <!-- Login Log Select 쿼리 -->
    <select id="findLoginLogAll" resultType="LogLogin">
        select * from LOG_LOGIN
    </select>  
    
</mapper>